    <asp:Button ID="Button1" runat="server" Text="Button" OnClick="Button1_Click" />
    <script type="text/javascript">
        // Script objects that should be loaded before we run
        var typeDependencies = ['Sys.Extended.UI.AlwaysVisibleControlBehavior'];
        
        // Reference to the Test Harness
        var testHarness = null;
    
        // Controls in the test page
        var panel = null;
        var btn = null;
        var begin = null;
        var end = null;
        var right = null;
        var behavior = null;

        // Constants
        var initialTop = 15;
        var initialLeft = 40;

        // Validate the initial location of the panel
        function checkInitialPosition() {
            testHarness.assertEqual(panel.offsetTop, initialTop, "Top of panel is at position " + panel.offsetTop + " instead of initial position " + initialTop);
            testHarness.assertEqual(panel.offsetLeft, initialLeft, "Left side of panel is at position " + panel.offsetLeft + " instead of initial position " + initialLeft);
        }

        // Validate that the panel has moved down from its original location
        function checkHasMovedVertical(position) {
                testHarness.assertNotEqual(panel.offsetTop, position, "Top of panel has not moved from position " + position);
        }

        // Validate that the panel has moved right from its original location
        function checkHasMovedHorizontal(position) {
                testHarness.assertNotEqual(panel.offsetLeft, position, "Left side of panel has not moved from position " + position);
        }
        
        // Scroll the page so that the element is visible
        function scrollIntoView(element) {
            return function() { element.scrollIntoView(); }
        }
        
        // Make sure the browser supports animation for this test (i.e. whether or not the browser
        // supports CSS position: fixed, or when a scrolleffect is desired)
        function requiresAnimation() {
        
			if (behavior._animate) {
				return;
			}

            testHarness.cancel('This test can only be run on a behavior that uses animation.');
        }

        // Register the AlwaysVisibleControl test cases
        function registerTests(harness) {
            testHarness = harness;

             // Get the controls on the page
            panel = testHarness.getElement("ctl00_ContentPlaceHolder1_content");
            btn = testHarness.getElement("ctl00_ContentPlaceHolder1_Button1");
            begin = testHarness.getElement("begin");
            end = testHarness.getElement("end");
            right = testHarness.getElement("right");
            
            // Get the ASP.NET AJAX objects on the page
            behavior = testHarness.getObject("avc1");
            
            var test = testHarness.addTest('Initial Position');
            test.addStep(checkInitialPosition);

            test = testHarness.addTest('Vertical Floating');
            test.addStep(checkInitialPosition);
            test.addStep(requiresAnimation);
            test.addStep(scrollIntoView(end), function() { try { checkHasMovedVertical(initialTop); return true; } catch (ex) { return false; } }, 200, 5000);
            test.addStep(scrollIntoView(begin), function() { try { checkHasMovedVertical(0); return true; } catch (ex) { return false; } }, 200, 5000);
            test.addPostBack(btn);
            test.addStep(checkInitialPosition);

            test = testHarness.addTest('Initial Position');
            test.addStep(checkInitialPosition);

            test = testHarness.addTest('Horizontal Floating');
            test.addStep(checkInitialPosition);
            test.addStep(requiresAnimation);
            test.addStep(scrollIntoView(right), function() { try { checkHasMovedHorizontal(initialLeft); return true; } catch (ex) { return false; } }, 200, 5000);

            test = testHarness.addTest('Positioning changes');
            test.addStep(checkInitialPosition);
            test.addStep(
                function() {
                    behavior.set_HorizontalSide(Sys.Extended.UI.HorizontalSide.Right);
                    behavior.set_HorizontalOffset(behavior.get_HorizontalOffset() * 2);
                    behavior.set_VerticalSide(Sys.Extended.UI.VerticalSide.Bottom);
                    behavior.set_VerticalOffset(behavior.get_VerticalOffset() * 2);
                    behavior.set_ScrollEffectDuration(behavior.get_ScrollEffectDuration() / 2);
                    behavior._reposition();
                },
                function() {
                    try {
                        checkHasMovedVertical(initialTop);
                        checkHasMovedHorizontal(initialLeft);
                        return true;
                    } catch (ex) { 
                        return false;
                    }
                },
                200, 5000);
        }                    
    </script>
